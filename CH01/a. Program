using System;

namespace Ch01
{
    class class1
    {
        public static void Main() // 메인이 여러개인 경우 실행이 되지 않는다.
                                  // 프로젝트 - 속성에서 시작개체를 설정할 수 있다. 
        {
            Console.WriteLine("class1 Main() 실행");
        }
    }

    class class2
    {
        public static void Main()
        {
            Console.WriteLine("class2 Main() 실행");
        }
    }
}

// 속성 창에서 컴파일러 작업 없음으로 설정하여 빌드에서 제외
